{
  "title": "存鱼进销存",
  "description": "存鱼进销存",
  "language" : "zh-hans",
  "author" : "存鱼进销存",
  "plugins": [
    "-highlight",
    "-sharing",
    "splitter",
    "-lunr",
    "-search",
    "search-pro",
    "expandable-chapters-small",
    "toggle-chapters",
    "theme-comscore",
    "favicon",
    "insert-logo",
    "back-to-top-button",
    "page-copyright",
    "hide-element",
    "lightbox",
    "addcssjs",
    "page-toc-button"
  ],
  "pluginsConfig": {
    "links": {
      "gitbook": false
    },
    "sharing": {
      "google": false,
      "facebook": false,
      "twitter": false,
      "all": false
    },
    "sitemap": {
      "hostname": "https://www.cunyu.co/"
    },
    "expandable-chapters-small":{},
    "favicon": {
      "shortcut": "favicon.ico",
      "bookmark": "favicon.ico"
    },
    "insert-logo": {
      "url": "https://x-account.oss-cn-shenzhen.aliyuncs.com/cdn/logo/logo1/yellow/03.png",
      "style": "background: none; max-height: 50px; min-height: 50px"
    },
    "page-copyright": {
      "description": "修订时间：",
      "signature": "存鱼进销存",
      "wisdom": "(<a href='https://www.cunyu.co/' target='_blank'>存鱼进销存</a>-操作指南)",
      "format": "YYYY-MM-dd hh:mm:ss",
      "copyright": "Copyright &#169; 存鱼进销存",
      "timeColor": "#666",
      "copyrightColor": "#666",
      "utcOffset": "8",
      "style": "normal",
      "isShowQRCode": false,
      "isShowIssues": false,
      "noPowered": true
    },

    "addcssjs": {
      "js": [
        "js/jweixin-1.6.0.js",
        "js/cunyu.js"
      ],
      "css": [
        "styles/bootstrap.min.css",
        "styles/freelancer.css"
      ]
    },
    "hide-element": {
      "elements": [".gitbook-link"]
    },
    "lightbox": {
      "includeJQuery": false,
      "sameUuid": true
    }
  }
}
